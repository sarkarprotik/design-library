{
  "sasForm": [
    {
      "heading": "Bli sportkund",
      "action": "/sendMe.html",
      "method": "post",
      "submitLabel": "Skicka",
      "fieldSets": [
        {
          "heading": "Företags- eller klubbinformation",
          "fields": [
            {
              "name": "organisationname",
              "label": "Företags-/ föreningsnamn",
              "mandatory": true,
              "class": "long first",
              "dataValidation": "^\\S{2,}",
              "errorMessage": "Vv fyll i namn"
            },
            {
              "name": "organisationNumber",
              "label": "Org nummer",
              "mandatory": true,
              "class": "short",
              "dataValidation": "^[\\d-]{10,12}$",
              "errorMessage": "Vv fyll i siffror"
            },
            {
              "name": "address",
              "label": "Företagsadress",
              "mandatory": true,
